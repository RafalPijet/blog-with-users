{
  "name": "blog-with-users",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:RafalPijet/blog-with-users.git",
  "author": "Rafal Pijet <stronglopez@wp.pl>",
  "license": "MIT",
  "dependencies": {
    "npm-run-all": "^4.1.5"
  },
  "scripts": {
    "start": "npm-run-all -p -l start:server",
    "start:dev": "npm-run-all -p -l start:*",
    "start:server": "cd server && yarn start",
    "start:client": "cd client && yarn start",
    "build": "run-p build:*",
    "build:clien": "cd client && npm run build",
    "install:client": "cd client && npm install",
    "install:server": "cd server && npm install",
    "postinstall": "run-p install:*"
  }
}
